{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "dev-expression",
    [
      "@babel/proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
